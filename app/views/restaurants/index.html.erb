<div class="banner text-center" style="background-image: linear-gradient(135deg, rgba(0,0,0,0.8) 0%, rgba(0,0,0,0.2) 50%), url('data:image/jpeg;base64,');
;"><h1>Welcome on FakeYelp</h1></div>


  <h2 class="text-center">Restaurants</h2>

  <p class="text-center"><%= link_to 'Create a new restaurant', new_restaurant_path %></p>

  <div class="container">
    <div class="row">
      <% @restaurants.each do |restaurant| %>
        <div class="col-sm-6">
          <div class="card">
            <div class="card-content">
              <h3> <%= link_to restaurant.name, restaurant_path(restaurant) %></h3>
              <p>Address: <%= restaurant.address %></p>
              <p>Phone number: <%= restaurant.phone_number %></p>
              <p>Category: <%= restaurant.category %></p>
            </div>
            <%= link_to "Reviews", restaurant_path(restaurant) , class:"card-review btn btn-primary btn-lg"%>
          </div >
        </div>
      <% end%>
    </div>
  </div>
